{
  "name": "setup-ocaml",
  "version": "0.0.0",
  "author": "Sora Morimoto <sora@morimoto.io>",
  "description": "GitHub Action for the OCaml programming language",
  "license": "ISC",
  "private": true,
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
    "build:index": "ncc build src/index.ts --license=LICENSE.txt --v8-cache",
    "build:post": "ncc build src/post.ts --out=dist/post --license=LICENSE.txt --v8-cache",
    "build": "npm-run-all -p build:*"
  },
  "dependencies": {
    "@actions/cache": "1.0.5",
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0",
    "@actions/http-client": "1.0.9",
    "@actions/io": "1.0.2",
    "@actions/tool-cache": "1.6.1",
    "cheerio": "1.0.0-rc.5",
    "semver": "7.3.4"
  },
  "devDependencies": {
    "@types/node": "14.14.21",
    "@types/semver": "7.3.4",
    "@typescript-eslint/eslint-plugin": "4.13.0",
    "@typescript-eslint/parser": "4.13.0",
    "@vercel/ncc": "0.27.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "typescript": "4.1.3"
  }
}
